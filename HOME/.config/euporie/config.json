{
  "color_scheme": "custom",
  "syntax_theme": "gruvbox-dark",
  "custom_background_color": "#282828",
  "custom_foreground_color": "#ebdbb2",
  "graphics": "kitty",
  "force_graphics": true,
  "custom_styles": {
    "prompt": "fg: #fb4934 bold"
  },
  "notebook": {
    "show_side_bar": true,
    "always_show_tab_bar": true,
    "expand": true,
    "edit_mode": "vi",
    "enable_language_servers": true,
    "autoformat": true,
    "autocomplete": true
  }
}

